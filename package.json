{
	"name": "easings.net",
	"version": "1.0.0",
	"main": "gulpfile.js",
	"author": "Ivan Solovev <solovev.one@yandex.ru> (https://solovev.one/)",
	"scripts": {
		"start": "parcel ./src/index.pug",
		"build": "NODE_ENV=production node ./build.js",
		"precommit": "lint-staged"
	},
	"browserslist": [
		"defaults",
		"not ie 11"
	],
	"postcss": {
		"plugins": {
			"postcss-pxtorem": {
				"rootValue": 16,
				"mediaQuery": true,
				"propList": [
					"font",
					"font-size",
					"line-height",
					"letter-spacing",
					"margin",
					"padding"
				]
			},
			"postcss-flexbugs-fixes": {},
			"autoprefixer": {}
		}
	},
	"license": "MIT",
	"dependencies": {
		"focus-visible": "^4.1.5",
		"parcel-bundler": "^1.12.2"
	},
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"autoprefixer": "^9.5.0",
		"css-mqpacker": "^7.0.0",
		"husky": "^1.3.1",
		"js-yaml": "^3.12.2",
		"lint-staged": "^8.1.5",
		"parcel-plugin-compress": "^1.0.7",
		"postcss": "^7.0.14",
		"postcss-custom-properties": "^8.0.9",
		"postcss-flexbugs-fixes": "^4.1.0",
		"postcss-preset-env": "^6.6.0",
		"postcss-pxtorem": "^4.0.1",
		"posthtml": "^0.11.3",
		"prettier": "^1.16.4",
		"pug": "^2.0.3",
		"sass": "^1.17.2",
		"terser": "^3.17.0",
		"tslint": "^5.14.0",
		"typescript": "^3.3.3333"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"compress": {
		"test": ".",
		"gzip": {
			"enabled": true,
			"numiterations": 15,
			"blocksplitting": true,
			"blocksplittinglast": false,
			"blocksplittingmax": 15
		},
		"brotli": {
			"enabled": true,
			"mode": 0,
			"quality": 11,
			"lgwin": 22,
			"lgblock": 0,
			"enable_dictionary": true,
			"enable_transforms": false,
			"greedy_block_split": false,
			"enable_context_modeling": false
		}
	}
}
